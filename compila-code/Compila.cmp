program CompilaCmp
begin
  class Complex
    begin
        var Real : float;
        var Imag : float;
    end;
  var dummy : Complex;

  proc Add(x : int, y : int): int
  begin
    var i : int;
    var j : float;
    var lit : bool;
    i := 5;
    while (i > 0) do
      begin
      i := i - 1;
      end;
    i := i + 10;
    i := i - 1;
    i := i * 2;
    i := i / 2;
    //j := 1.0 # 2.0;
    lit := true;
    if (lit) then
      begin
      i := 0;
      end
    else
      begin
      i := 1;
      end;
    return i + y;
  end;

  proc Main()
  begin
    printint(Add(1, 2));
    return;
  end;
end;
